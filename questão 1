#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    int A, B, C, D, res;

    scanf ("%d",&A);
    scanf ("%d",&B);
    scanf ("%d",&C);
    scanf ("%d",&D);

    printf("DIFERENCA = (%d * %d - %d * %d)\n",A,B,C,D);
    res=A * B - C * D;
    printf("DIFERENCA = %d"), res;

   return 0;
}
