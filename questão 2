#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    float x1,x2,y1,y2,res;
    float distancia;

    printf ("x1:");
    scanf ("%f",&x1);
    printf ("y1:");
    scanf ("%f",&y1);
    printf ("x2:");
    scanf ("%f",&x2);
    printf ("y2:");
    scanf ("%f",&y2);

    res = sqrt( pow(x2-x1, 2) + pow(y2-y1, 2) );

    printf("%.4f", res);
    
    return 0;
}
