#include <stdio.h>
#include <stdlib.h>
#include <math.h>


int main()
{
    float R,area,vol;
    const float pi = 3.14159f;

    printf ("Raio:");
    scanf ("%f",&R);

    area=(4.0*pi*R*R*R)/3.0;
    vol=4*pi*R*R;
    
    printf("Area %f\n", area);
    printf("Volume %f", vol);
    
    return 0;
}
